#include <stdio.h>
#define MAX 100
int graph[MAX][MAX];   
int visited[MAX];      
int n;                 
void DFS(int vertex) {
    printf("%d ", vertex);
    visited[vertex] = 1;
    int i,j;

    for (i = 0; i < n; i++) {
        if (graph[vertex][i] == 1 && !visited[i]) {
            DFS(i);
        }
    }
}
int main() {
    int i,j,edges, src, dest;

    printf("Enter number of vertices: ");
    scanf("%d", &n);
    // Initialize graph and visited array
    for (i = 0; i < n; i++) {
        visited[i] = 0;
        for (j = 0; j < n; j++) {
            graph[i][j] = 0;
        }
    }
    printf("Enter number of edges: ");
    scanf("%d", &edges);
// Input edges
    for (i = 0; i < edges; i++) {
        printf("Enter edge (source destination): ");
        scanf("%d%d", &src, &dest);
        graph[src][dest] = 1;
        graph[dest][src] = 1; 
    int startVertex;
    printf("Enter starting vertex for DFS: ");
    scanf("%d", &startVertex);
    printf("DFS traversal starting from vertex %d:\n", startVertex);
    DFS(startVertex);
    return 0;
}
