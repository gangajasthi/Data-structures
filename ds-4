#include <stdio.h>

int main() {
    int arr[] = {3, 5, 7, 9, 11, 13, 17, 19};  
    int target = 13;
	int i;  
         int n = sizeof(arr) / sizeof(arr[0]);
    int found = 0;  
    for (i = 0; i < n; i++) {
        if (arr[i] == target) {
            found = 1;  
            printf("Element %d found at index %d.\n", target, i);
            break;  
        }
    }
    if (!found) {
        printf("Element %d not found in the array.\n", target);
    }
    
    return 0;
}
